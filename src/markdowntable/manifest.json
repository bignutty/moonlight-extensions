{
  "$schema": "https://moonlight-mod.github.io/manifest.schema.json",
  "id": "markdowntable",
  "version": "1.0.0",
  "meta": {
    "name": "Markdown Table",
    "tagline": "adds markdown table formatting",
    "authors": ["bignutty"],
    "tags": ["chat"]
